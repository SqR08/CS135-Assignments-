;;3a)
;;A Nat3 is one of:
;;0, 1, 3
;;(add3 Nat)

;;3b)
(define (my-nat3-fn n)
  (cond
    [(zero? n)...]
    [(= 1 n)...]
    [(= 2 n)...]
    [else (... (my-nat-fn (sub1 n))...)]))